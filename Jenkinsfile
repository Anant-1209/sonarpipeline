node {
    stage('Checkout') {
        echo ''  
    }
    
    stage('Build') {
        echo ''  
    }
    
    stage('Test') {
        echo 'Running tests...'
          
    }
    
    stage('Deploy') {
        echo 'Deploying the project...'
          
    }
  if(currentBuild.result=='SUCCESS')
{
  echo 'looks good'
}
  else{
    echo 'failed'
  }
}
